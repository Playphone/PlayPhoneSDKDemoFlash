<?xml version="1.0"?>
<hcode:RoomCookies xmlns:fx="http://ns.adobe.com/mxml/2009"
          xmlns:s="library://ns.adobe.com/flex/spark"
          xmlns:hcode="hcode.*"
          width="100%"
          paddingLeft="5" paddingTop="5" paddingRight="5" gap="5">
    <s:Label>Put data key(integer value) here</s:Label>
    <s:TextInput id="room_cookie_key" width="100%"/>

    <s:Label>Type some text to save cloud storage</s:Label>
    <s:TextInput id="room_cookie_val" width="100%"/>

    <s:Button id="btnUpload" width="100%">Upload data</s:Button>

    <s:Label>Put data key(integer value) here for download</s:Label>
    <s:TextInput id="download_cookie_key" width="100%"/>
    <s:Button id="btnReload" width="100%">Reload data</s:Button>
</hcode:RoomCookies>